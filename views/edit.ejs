<html>
    <head>
       <title>Edit|Employee</title>
       <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js" type="text/javascript"></script>
          <script src="/assets/list.js" type="text/javascript"></script>
          <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
    </head>
    <body>

          <div id = "Emp table" class="container">
           <form class="form-horizontal">
              Emp Id :<br/>
             <input type="text" id="id" class="form-control" value="<%= data[0].empid %>" readonly /><br/>
             Employee Name : <br/>
            <input type="text" id="EmpName" class="form-control" value="<%= data[0].name %>" /><br/>
            Designation :<br/>
            <input type="text" id="Empdesg" class="form-control" value="<%= data[0].designation %>" /><br/>
            Manager : <br/>
            <input type="text" id="EmpMan" class="form-control" value="<%= data[0].manager %>" /><br/>
            <button id="edit" type="reset" class="btn btn-default" >Edit</button><br/>

           <br/>
          </form>
            </div>


    </body>
</html>
